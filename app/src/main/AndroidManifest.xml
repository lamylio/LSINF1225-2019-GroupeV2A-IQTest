<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="be.uclouvain.lsinf1225.groupev2a.iqtest">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo_puzzle"
        android:label="@string/APP_NAME"
        android:roundIcon="@drawable/app_logo_puzzle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".Controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controller.RegisterActivity"/>
        <activity android:name=".Controller.Game.GameActivity"/>
        <activity android:name=".Controller.ProfileActivity" />

        <activity android:name=".Controller.HistoryActivity"></activity>

        <activity android:name=".Controller.Game.CategoryActivity" />
        <activity android:name=".Controller.Game.NormalActivity" />
        <activity android:name=".Controller.HistoryActivity$ChooseModeActivity" />
    </application>

</manifest>